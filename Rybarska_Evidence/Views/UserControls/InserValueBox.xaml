<UserControl x:Class="Rybarska_Evidence.Views.UserControls.InserValueBox"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Rybarska_Evidence.Views.UserControls"
             mc:Ignorable="d"  x:Name="intertValueBox"
             d:DesignHeight="70" d:DesignWidth="320">
    
    <Grid Margin="10">
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
            <TextBlock></TextBlock>

        <TextBlock Grid.Row="0" Text="{Binding ElementName=intertValueBox, Path=Caption}"></TextBlock>
        <TextBox Grid.Row="1" ></TextBox>

    </Grid>
</UserControl>
