<Window x:Class="Rybarska_Evidence.Views.UserControls.AddNewGround"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Rybarska_Evidence.Views.UserControls"
        mc:Ignorable="d"
        Title="AddNewGround" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>

        </Grid.RowDefinitions>
        <TextBlock Grid.Row="0"/>
        <TextBlock Grid.Row="1"/>
        <TextBlock Grid.Row="2"/>
        <TextBlock Grid.Row="3"/>
        <ComboBox Grid.Row="4"/>
        <TextBlock Grid.Row="5"/>
        <Button Grid.Row="6" Content="Pridat" Command="{Binding AddNewGroundCommand}"/>

    </Grid>
</Window>
